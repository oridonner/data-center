      <Value>YES</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="TargetDatabase">
      <Value>DataCenter</Value>
    </SqlCmdVariable>
  </ItemGroup>
<Target Name="test">
  <Message Text="_gitlab_project : $(_gitlab_project)"/>
</Target>
</Project>