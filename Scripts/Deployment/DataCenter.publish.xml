      <Value>IT-DEV-01\DataCenter</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="PriorityDatabase">
      <Value>cele</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="PriorityServer">
      <Value>APC-DB1\TEST</Value>
    </SqlCmdVariable>
